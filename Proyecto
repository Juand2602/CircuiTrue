class CircuiTrue:

  def iniVariables(variable):
    variable.entradas(0,0,0) # Por favor coloque ceros como tantas variables desee tener
  
  def entradas(variable,p,q,r): # Inicialice sus variables siguiendo el siguiente formarto: variable."Aquí ponga el nombre de su variable" = variable.
    variable.p = p              # Es estrictamente necesario que inicialice todas las variables que definió en inivariables es decir que sí coloco 5 ceros
    variable.q = q              # Inicialice 5 variables
    variable.r = r
    variable.calcular()
  
  def calcular(variable): #Truco para una función que no queremos implementar
    pass
  
  def salida(variable):
    return variable.s
